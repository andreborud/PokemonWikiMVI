<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">

    <gradient
        android:angle="90"
        android:endColor="#D3D3D3"
        android:startColor="#F6F6F6"
        android:type="linear" />

    <stroke
        android:color="#272727"
        android:width="3dp" />

    <size
        android:width="96dp"
        android:height="96dp"/>

    <padding
        android:bottom="16dp"
        android:top="16dp"
        android:left="16dp"
        android:right="16dp"/>

</shape>